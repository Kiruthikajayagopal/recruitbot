<div style="text-align:center" class="container">
  <h3 style="color:green">Data you entered</h3>


  <table class="table table-bordered">
    <thead>
      <tr>
        <th>
          <b>Number</b>
        </th>
        <th>
          <b>Question</b>
        </th>
        <th>
          <b>Option 1</b>
        </th>
        <th>
          <b>Option 2</b>
        </th>
        <th>
          <b>Option 3</b>
        </th>
        <th>
          <b>Option 4</b>
        </th>
        <th>
          <b>Correct Option</b>
        </th>
         <th>
          <b>Category</b>
        </th>
         <th>
          <b>Level</b>
        </th>
      </tr>
    </thead>
    <tbody>
      {{# each items }}
      <tr>

        <td>{{ this.num }}</td>
        <td>{{ this.question }}</td>
        <td> {{ this.option1 }}</td>
        <td> {{ this.option2 }}</td>
        <td> {{ this.option3 }}</td>
        <td> {{ this.option4}} </td>
        <td> {{ this.optionCorrect }}</td>
        <td> {{this.category}}</td>
        <td>{{this.level }}</td>

      </tr>

      {{/each}}
      <tr>
    </tbody>
  </table>
</div>

<table border='0' cellpadding='0' cellspacing='0' width='700px' align='center'>
  <tr>
    <td align='center'>
      <input type="button" onclick="history.back(-1)" class="back" name='back' value="BACK">
      <br>
      </br>
      <br>
      </br>
    </td>
  </tr>
</table>
<style>
  .back {
    background-image: url(../images/arrow.png);
    background-repeat: no-repeat;
    background-position: 300px;
    width: 200px;
    padding: 12px;
    margin-top: 8px;
    font-family: "Comic Sans MS", "Comic Sans";
    cursor: pointer;
    line-height: 1;
    border-radius: 5px;
    font-size: 20px;
    -webkit-appearance: none;
    box-shadow: inset 0 0 8px 0 rgba(0, 0, 0, 0.4);
    outline: none
  }

  .back:hover {
    background-color: #36b051;
    color: #fff;
  }
</style>
